v-slot:item.actions=\"{ item }\">\n<!--            <v-icon-->\n<!--                small-->\n<!--                class=\"mr-2\"-->\n<!--                @click=\"namespaceModifyShow(item)\"-->\n<!--            >-->\n<!--              mdi-pencil-->\n<!--            </v-icon>-->\n            <v-icon\n                small\n                @click=\"namespaceDeleteShow(item)\"\n            >\n              mdi-delete\n            </v-icon>\n\n          </template>\n\n        </v-data-table>\n      </v-card>\n\n\n      <br>\n    </div>\n\n    <v-dialog\n        v-model=\"modifyDialog\"\n        max-width=\"600px\"\n    >\n      <v-card>\n        <v-card-title>\n          <span class=\"headline\"> 네임스페이스 수정 </span>\n        </v-card-title>\n\n        <v-card-text>\n          <v-container>\n            <v-row>\n              <v-col\n                  cols=\"12\"\n                  sm=\"6\"\n                  md=\"4\"\n              >\n                <v-text-field\n                    v-model=\"editTable.name\"\n                    label=\"네임스페이스 이름\"\n                ></v-text-field>\n              </v-col>\n            </v-row>\n          </v-container>\n        </v-card-text>\n\n<!--        <v-card-actions>-->\n<!--          <v-spacer></v-spacer>-->\n<!--          <v-btn-->\n<!--              color=\"blue darken-1\"-->\n<!--              text-->\n<!--              @click=\"modifyNamespaceName\"-->\n<!--          >-->\n<!--            저장-->\n<!--          </v-btn>-->\n<!--          <v-btn-->\n<!--              color=\"blue darken-1\"-->\n<!--              text-->\n<!--              @click=\"closeModify\"-->\n<!--          >-->\n<!--            취소-->\n<!--          </v-btn>-->\n<!--        </v-card-actions>-->\n      </v-card>\n    </v-dialog>\n    <v-dialog v-model=\"deleteDialog\" max-width=\"500px\">\n      <v-card>\n        <v-card-title class=\"headline\">네임스페이스를 삭제하시겠습니까?</v-card-title>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn colo